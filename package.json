{
    "name": "opencapture",
    "version": "3.5.0",
    "license": "GPL-3.0",
    "author": "Edissyum Consulting <nathan.cheval@edissyum.com>",
    "description": "Open-Capture is the one and only 100% Open Source intelligent document capture solution",
    "engines": {
        "npm": ">= 9.2.0",
        "node": ">= 22.2.0"
    },
    "scripts": {
        "check-update": "ncu",
        "build-prod": "ng build --configuration production",
        "build-watch": "ng build --configuration development --watch",
        "upgrade-all-packages-ncu": "ncu -u && npm run reload-packages",
        "reload-packages": "rm -Rf node_modules && rm -f package-lock.json && npm i --force",
        "extract-translations": "ngx-translate-extract --input ./src/frontend/ --output ./src/assets/i18n/frontend/*.json --clean --sort --format namespaced-json --marker _"
    },
    "private": true,
    "dependencies": {
        "@ngstack/code-editor": "^9.0.0",
        "@swimlane/ngx-charts": "^22.0.0",
        "@tinymce/tinymce-angular": "^8.0.1",
        "moment": "^2.30.1",
        "ng-sortgrid": "^19.0.0",
        "ng2-pdf-viewer": "^10.4.0",
        "ngx-file-drag-drop": "^11.0.0",
        "ngx-mat-select-search": "^8.0.1",
        "ngx-toastr": "^19.0.0",
        "remove-accents": "^0.5.0",
        "svg-country-flags": "^1.2.10",
        "tinymce": "^7.9.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.13",
        "@angular-devkit/build-webpack": "^0.1902.13",
        "@angular-material-extensions/select-country": "^17.0.0",
        "@angular/animations": "^19.2.12",
        "@angular/cdk": "^19.2.17",
        "@angular/cli": "^19.2.13",
        "@angular/common": "^19.2.12",
        "@angular/compiler": "^19.2.12",
        "@angular/compiler-cli": "^19.2.12",
        "@angular/core": "^19.2.12",
        "@angular/forms": "^19.2.12",
        "@angular/material": "^19.2.17",
        "@angular/material-moment-adapter": "^19.2.17",
        "@angular/platform-browser": "^19.2.12",
        "@angular/platform-browser-dynamic": "^19.2.12",
        "@angular/router": "^19.2.12",
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@ngx-translate/core": "^16.0.4",
        "@ngx-translate/http-loader": "^16.0.1",
        "@types/d3-scale": "^4.0.9",
        "@types/d3-selection": "^3.0.11",
        "@types/d3-shape": "^3.1.7",
        "@types/node": "^22.15.21",
        "@vendure/ngx-translate-extract": "^9.4.0",
        "eslint": "^9.27.0",
        "jquery": "^3.7.1",
