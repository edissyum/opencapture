{
    "name": "opencapture",
    "version": "3.5.4",
    "license": "GPL-3.0",
    "author": "Edissyum Consulting <nathan.cheval@edissyum.com>",
    "description": "Open-Capture is the one and only 100% Open Source intelligent document capture solution",
    "engines": {
        "npm": ">= 11.3.0",
        "node": ">= 24.0.0"
    },
    "scripts": {
        "check-update": "ncu",
        "build-prod": "ng build --configuration production",
        "build-watch": "ng build --configuration development --watch",
        "upgrade-all-packages-ncu": "ncu -u && npm run reload-packages",
        "reload-packages": "rm -Rf node_modules && rm -f package-lock.json && npm i --force",
        "extract-translations": "ngx-translate-extract --input ./src/frontend/ --output ./src/assets/i18n/frontend/*.json --clean --sort --format namespaced-json --marker _"
    },
    "private": true,
    "dependencies": {
        "@ngstack/code-editor": "^9.0.0",
        "@swimlane/ngx-charts": "^23.0.0-alpha.0",
        "@tinymce/tinymce-angular": "^9.1.0",
        "moment": "^2.30.1",
        "ng-sortgrid": "^19.0.0",
        "ng2-pdf-viewer": "^10.4.0",
        "ngx-file-drag-drop": "^11.0.0",
        "ngx-mat-select-search": "^8.0.2",
        "ngx-toastr": "^19.0.0",
        "remove-accents": "^0.5.0",
        "svg-country-flags": "^1.2.10",
        "tinymce": "^8.0.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^20.1.4",
        "@angular-devkit/build-webpack": "^0.2001.4",
        "@angular-material-extensions/select-country": "^17.0.0",
        "@angular/animations": "^20.1.4",
        "@angular/cdk": "^20.1.4",
        "@angular/cli": "^20.1.4",
        "@angular/common": "^20.1.4",
        "@angular/compiler": "^20.1.4",
        "@angular/compiler-cli": "^20.1.4",
        "@angular/core": "^20.1.4",
        "@angular/forms": "^20.1.4",
        "@angular/material": "^20.1.4",
        "@angular/material-moment-adapter": "^20.1.4",
        "@angular/platform-browser": "^20.1.4",
        "@angular/platform-browser-dynamic": "^20.1.4",
        "@angular/router": "^20.1.4",
        "@fortawesome/fontawesome-free": "^7.0.0",
        "@ngx-translate/core": "^17.0.0",
        "@ngx-translate/http-loader": "^17.0.0",
        "@types/d3-scale": "^4.0.9",
        "@types/d3-selection": "^3.0.11",
        "@types/d3-shape": "^3.1.7",
        "@types/node": "^24.1.0",
        "@vendure/ngx-translate-extract": "^10.0.1",
        "eslint": "^9.32.0",
